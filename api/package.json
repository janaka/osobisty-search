    "@types/hapi__joi": "^17.1.7",
    "@types/hapi__vision": "^5.5.3",
    "@types/jest": "^27.0.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "babel-jest": "^27.3.1",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "data/*",
      ".dccache",
      "README.md"
