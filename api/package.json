    "@hapi/hapi": "^20.2.1",
    "@hapi/inert": "^6.0.4",
    "@hapi/vision": "^6.1.0",
    "@hapi/wreck": "^17.1.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "good": "^8.1.2",
    "hapi-auth-jwt2": "^10.2.0",
    "hapi-swagger": "^14.2.4",
    "joi": "^17.4.2",
    "jwks-rsa": "^2.0.5",
    "morgan": "^1.10.0",
    "typescript": "^4.4.4"
